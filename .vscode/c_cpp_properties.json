{
    "configurations": [
        {
            "name": "Windows Vulkan",
            "defines": [
                "WIN64",
                "_WINDOWS",
                "_CRT_SECURE_NO_WARNINGS",
                "DEBUG",
                "USE_LOGGER",
                "_WINAPI",
                "UNICODE",
                "_UNICODE",
                "VULKAN_RENDER",
                "USE_ASSIMP",
                "USE_STB",
                "USE_GLI",
                "USE_SPIRV"
            ],
            "includePath": [
                "${workspaceFolder}/Engine/Source",
                "${workspaceFolder}/Engine/Libs"
            ],
            "intelliSenseMode": "msvc-x64",
            "cppStandard": "c++17",
            "macFrameworkPath": [],
            "compilerArgs": [
                "/std:c++17"
            ],
            "cStandard": "c17",
            "intelliSenseModeIsExplicit": true,
            "cStandardIsExplicit": true,
            "cppStandardIsExplicit": true,
            "browse": {
                "path": [
                    "${workspaceFolder}/Engine/Source",
                    "${workspaceFolder}/Engine/Libs",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "Windows Direct3D",
            "defines": [
                "WIN64",
                "_WINDOWS",
                "_CRT_SECURE_NO_WARNINGS",
                "DEBUG",
                "USE_LOGGER",
                "_WINAPI",
                "UNICODE",
                "_UNICODE",
                "D3D_RENDER",
                "USE_ASSIMP",
                "USE_STB",
                "USE_GLI"
            ],
            "includePath": [
                "${workspaceFolder}/Engine/Source",
                "${workspaceFolder}/Engine/Libs"
            ],
            "intelliSenseMode": "msvc-x64",
            "cppStandard": "c++17",
            "macFrameworkPath": [],
            "compilerArgs": [
                "/std:c++17"
            ],
            "cStandard": "c17",
            "intelliSenseModeIsExplicit": true,
            "cStandardIsExplicit": true,
            "cppStandardIsExplicit": true,
            "browse": {
                "path": [
                    "${workspaceFolder}/Engine/Source",
                    "${workspaceFolder}/Engine/Libs",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "Android Vulkan",
            "defines": [
                "__ANDROID__",
                "_ANDROID",
                "DEBUG",
                "USE_LOGGER",
                "UNICODE",
                "_UNICODE",
                "VULKAN_RENDER",
                "VULKAN_DYNAMIC",
                "USE_ASSIMP",
                "USE_STB",
                "USE_GLI",
                "USE_SPIRV",
                "USE_HWC_PIPE"
            ],
            "includePath": [
                "${workspaceFolder}/Engine/Source",
                "${workspaceFolder}/Engine/Libs",
                "${env:ANDROID_NDK_HOME}/sysroot/usr/include"
            ],
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "cStandard": "c17",
            "compileCommands": "${workspaceFolder}/Engine/Libs/spirv/shaderc/build_android/Debug/compile_commands.json"
        }
    ],
    "version": 4
}